{
    "name": "chat",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migrate": "pnpx prisma db push",
        "seed": "pnpx prisma db seed -- --delete "
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.577.0",
        "@aws-sdk/s3-request-presigner": "^3.577.0",
        "@nestjs/common": "^9.4.3",
        "@nestjs/config": "^2.3.4",
        "@nestjs/core": "^9.4.3",
        "@nestjs/platform-express": "^9.4.3",
        "@nestjs/platform-socket.io": "^10.3.8",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/terminus": "^10.2.3",
        "@nestjs/websockets": "^10.3.8",
        "@prisma/client": "4.15.0",
        "@sentry/node": "^8.7.0",
        "@socket.io/redis-adapter": "^8.3.0",
        "axios": "^1.7.2",
        "jsonwebtoken": "^9.0.2",
        "open-graph-scraper": "^6.7.1",
        "openid-client": "^5.6.5",
        "redis": "^4.6.14",
        "rxjs": "^7.8.1",
        "socket.io": "^4.7.5",
        "source-map-support": "^0.5.21",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.5.0",
        "@nestjs/schematics": "^9.2.0",
        "@nestjs/testing": "^9.4.3",
        "@types/express": "^4.17.17",
        "@types/jest": "29.5.0",
        "@types/multer": "^1.4.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "29.5.0",
        "prettier": "^2.8.8",
        "prisma": "^4.16.2",
        "supertest": "^6.3.3",
        "ts-jest": "29.0.5",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "^4.9.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.(test|spec)\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/test/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/keycloak(|/.*)$": "<rootDir>/libs/keycloak/src/$1",
            "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
            "^src(|/.*)$": "<rootDir>/src/$1"
        }
    },
    "prisma": {
        "schema": "libs/common/src/database/schema.prisma",
        "seed": "ts-node libs/common/src/database/seeds/index.ts"
    },
    "packageManager": "pnpm@9.1.3+sha512.7c2ea089e1a6af306409c4fc8c4f0897bdac32b772016196c469d9428f1fe2d5a21daf8ad6512762654ac645b5d9136bb210ec9a00afa8dbc4677843ba362ecd"
}
