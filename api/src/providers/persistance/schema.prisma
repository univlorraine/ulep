datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  id        String   @id @default(uuid())
  email     String   @unique
  createdAt DateTime @default(now())
}

model Media {
  id        String   @id @default(uuid())
  name      String
  bucket    String
  mime      String
  size      Int
  createdAt DateTime @default(now())
}