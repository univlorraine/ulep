{
  "name": "api",
  "version": "0.16.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --debug --tsc --watch",
    "start:debug": "nest start --debug --tsc --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "pnpx prisma migrate dev",
    "seed": "pnpx prisma db seed -- --delete ",
    "seed:ul": "pnpx prisma db seed -- --delete --seed ul",
    "seed:random": "pnpx prisma db seed -- --seed random --delete"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "4.15.0",
    "@sentry/node": "^7.78.0",
    "@sentry/tracing": "^7.78.0",
    "async": "^3.2.5",
    "axios": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-stringify": "^6.4.6",
    "date-fns-tz": "^2.0.1",
    "firebase-admin": "^12.0.0",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.5.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mjml": "^4.15.3",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.5",
    "openid-client": "^5.4.2",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/mustache": "^4.2.5",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "multer": "1.4.5-lts.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/keycloak(|/.*)$": "<rootDir>/libs/keycloak/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^src(|/.*)$": "<rootDir>/src/$1"
    }
  },
  "prisma": {
    "schema": "libs/common/src/database/schema.prisma",
    "seed": "ts-node libs/common/src/database/seeds/index.ts"
  },
  "packageManager": "pnpm@9.1.2+sha512.127dc83b9ea10c32be65d22a8efb4a65fb952e8fefbdfded39bdc3c97efc32d31b48b00420df2c1187ace28c921c902f0cb5a134a4d032b8b5295cbfa2c681e2"
